{
	"name": "guard-bot",
	"version": "0.0.1",
	"description": "",
	"main": "src-bot/index.js",
	"scripts": {
		"bot:dev": "ts-node src-bot/index.ts",
		"build": "tsc && pnpm _copy-files",
		"_copy-files": "cp .env ./dist && cp -r prisma dist",
		"bot:run": "node src/client/index.js",
		"clean": "rm -rf dist",
		"db:start": "mongod --dbpath='./data' --replSet='rs0'",
		"db:reload": "pnpx prisma generate",
		"db:prod": "node ./scripts/prisma.js",
		"deploy": "pnpm build && pnpm run db:prod && docker-compose build && docker-compose up -d",
		"switch": "node ./scripts/prod.js && pnpx prisma generate"
	},
	"author": "JockeRider199",
	"devDependencies": {
		"@types/express": "^4.17.16",
		"@types/jsonwebtoken": "^8.5.9",
		"@types/ms": "^0.7.31",
		"@types/node": "^18.11.18",
		"prettier": "^2.8.3",
		"prisma": "^4.11.0",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.4"
	},
	"dependencies": {
		"@prisma/client": "^4.11.0",
		"discord.js": "^14.8.0",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"jsonwebtoken": "^9.0.0",
		"ms": "^2.1.3"
	}
}
